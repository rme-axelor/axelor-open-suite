<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
   
   <grid model="com.axelor.apps.base.db.GroupAdvancedExport" title="Group advanced exports" name="group-advanced-export-grid" orderBy="sequence">
      <field name="sequence"/>
      <field name="name"/>
      <field name="advancedExport"/>
   </grid>
   
   <form model="com.axelor.apps.base.db.GroupAdvancedExport" title="Group advanced export" name="group-advanced-export-form">
      <panel name="advancedExportPanel">
         <field name="name" colSpan="12"/>
         <field name="advancedExport"/>
         <field name="sequence"/>
      </panel>
   </form>
</object-views>